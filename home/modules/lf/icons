
# vimft	conf
# Icons for lf

# default values from lf (with matching order)
# ln      l       # LINK
# or      l       # ORPHAN
# tw      t       # STICKY_OTHER_WRITABLE
# ow      d       # OTHER_WRITABLE
# st      t       # STICKY
# di      d       # DIR
# pi      p       # FIFO
# so      s       # SOCK
# bd      b       # BLK
# cd      c       # CHR
# su      u       # SETUID
# sg      g       # SETGID
# ex      x       # EXEC
# fi      -       # FILE

# file types (with matching order)
fi	📃	# FILE
di	📂	# DIR
tw	🤝	# STICKY_OTHER_WRITABLE
ow	📂	# OTHER WRITABLE
ln     "⛓ "  	# LINK
or	❌	# ORPHAN
ex	🎯	# EXEC
su      🙏      # SETUID
pi      📥      # FIFO
bd      💽      # BLK
so      "🖥️ "   # SOCK
st      💄      # STICKY

